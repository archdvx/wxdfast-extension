{
    "applications": {
        "gecko": {
            "id": "wxdfast@archdvx",
            "strict_min_version": "52.0"
        }
    },
    "offline_enabled": true,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": "icon_32.png",
        "default_popup": "popup.html"
    },
    "commands": {
        "toggle-interruption": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            },
            "description": "Toggle interruption"
        }
    },
    "description": "__MSG_extensionDescription__",
    "homepage_url": "https://wxdfast.dxsolutions.org",
    "icons": {
        "32": "icon_32.png",
        "48": "icon_48.png",
        "64": "icon_64.png",
        "128": "icon_128.png"
    },
    "manifest_version": 2,
    "minimum_chrome_version": "19.0",
    "name": "__MSG_extensionName__",
    "short_name": "wxdfast",
    "permissions": [
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "nativeMessaging",
        "contextMenus",
        "downloads",
        "storage",
        "cookies",
        "activeTab",
        "tabs"
    ],
    "version": "1.1.0",
    "default_locale": "en"
}
